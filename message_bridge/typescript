from RedisQueue import RedisQueue
import subprocess
import json
import base64

q = RedisQueue('job-identifier', namespace='ansible', host='localhost', port=6379, db=0)

q.put({
		'job': 'build',
		'build_id': 123,
		'call_id': 1234,
		'action': 'run_playbook',
		'with_vars': {
			'alpha': 1,
			'beta': 2
		}
	})

while True:
	res = q.get()
	print res
	print type(res)
	subprocess.Popen(["run_ansible_controller.sh", base64.b64encode(json.dumps(res))])
	print 'END'
